{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [],
  "extends": "standard",
  "rules": {
    "indent": ["error", 2, {"SwitchCase": 1}],
    "semi": ["error", "always"],
    "space-before-function-paren": "off",
    "padded-blocks": "off",
    "import/no-webpack-loader-syntax": "off",
    "lines-between-class-members": "off",
    "comma-dangle": "off"
  }
}
